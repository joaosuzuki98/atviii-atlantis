{
  "compilerOptions": {
    "target": "es2016",
    "module": "CommonJS",
    "outDir": "./out",
    "rootDir": "./", 
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules", "out"]
}